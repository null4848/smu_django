$(function () {
	var optionSelectWrap = $('.prod_detail_footer');
	$('.portrait_img_box', optionSelectWrap).each(function () {
		var imgTg = $(this);
		KyoboBookPub.ink.checkImagePortrait(imgTg);
	});


});

